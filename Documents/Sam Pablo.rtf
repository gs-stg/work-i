INSERT INTO `t_impuestopreguntas` (`idt_impuestoPreguntas`, `t_impuestoPreguntasTitulo`, `t_impuestoPreguntasTituloObservacion`, `t_impuestoPreguntasAyuda`, `t_impuestoPreguntasPreguntasObservacion`, `t_impuestoPreguntasRespuestasObservacion`) VALUES(12, '\'bfNos ha tra\'eddo los Certificados de Ingresos de las Todas las Empresas en las que ha trabajado?', NULL, 'Con Motivo de la Crisis se est\'e1 observando que muchas empresas NO declaran correctamente las retenciones o los ingresos y por tanto conviene chequear que los datos de ingresos y retenciones de la Informaci\'f3n Fiscal coinciden con los que efectivamente el trabajador ha cobrado, por ejemplo a trav\'e9s del Certificado de Retenciones', NULL, NULL);\
INSERT INTO `t_impuestopreguntas` (`idt_impuestoPreguntas`, `t_impuestoPreguntasTitulo`, `t_impuestoPreguntasTituloObservacion`, `t_impuestoPreguntasAyuda`, `t_impuestoPreguntasPreguntasObservacion`, `t_impuestoPreguntasRespuestasObservacion`) VALUES(13, '\'bfHa vuelto a trabajar en alguna empresa que le hubiera despedido en los 3 \'faltimos a\'f1os?', 'El Cliente ha vuelto a vincularse a la Empresa que le despidi\'f3 hace menos de 3 a\'f1os y se manifiesta: ', 'Si un trabajador vuelve en menos de TRES A\'d1OS a la empresa que le despidi\'f3, hay que informarle que la Indemnizaci\'f3n que en su d\'eda cobr\'f3 y que NO tribut\'f3 por ella, deja de estar EXENTA y por tanto debemos informarle que debe hacer una declaraci\'f3n complementaria del a\'f1o del despido y tributar por TODA la Indemnizaci\'f3n (Si esta indemnizaci\'f3n fue muy alta el Coste Fiscal ser\'e1 MUY GRANDE para el trabajador y por tanto debemos avisarle de ello)', '\'bfLa Empresa que le Despidi\'f3 hace menos de 3 a\'f1os es la MISMA o tiene los mismos Socios o la misma Direcci\'f3n que la que le ha vuelto a Contratar?@\'bfHa vuelto a trabajar para la Empresa que le Despidi\'f3 como Aut\'f3nomo? (Tampoco hay desvinculaci\'f3n)', 'Se informa al Cliente que la Indemnizaci\'f3n que percibi\'f3 es su d\'eda NO ESTA EXENTA y debe Regularizar la Renta del a\'f1o que cobr\'f3 la Indemnizaci\'f3n para Tributar por ella');\
